#include <bits/stdc++.h>
using namespace std;
typedef long long li;
typedef double di;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    li n;
    cin >> n;
    li sum = (n * (n + 1)) / 2;
    for (int i = 1; i < n; i++)
    {
        li a;
        cin >> a;
        sum -= a;
    }
    cout << sum << endl;
}
