#include <bits/stdc++.h>
using namespace std;
#define li long long
const li N = 1e9 + 7;

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    // int t_case; cin>>t_case; while(t_case--){           }
    li n;
    cin >> n;

    li a = 5;
    li sum = 0;
    while (a <= n)
    {
        sum += n / a;
        a *= 5;
    }
    cout << sum << endl;
}
